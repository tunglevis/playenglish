a:2:{i:0;a:7:{s:2:"id";s:2:"13";s:5:"title";s:24:"Kê tay làm gối ngủ";s:4:"desc";s:265:"Khi ngủ, 2 tay kê làm gối dưới đầu, ngoài ảnh hưởng đến tuần hoàn máu, gây ra đau nhức, tê mỏi ở chi trên, mà còn làm cho áp lực trong bụng tăng cao, thời gian dài sẽ gây ra “viêm thực quản trào ngược”.";s:7:"content";s:0:"";s:7:"created";s:8:"17/11/13";s:4:"type";s:7:"SUCKHOE";s:10:"type_label";s:12:"Sức khỏe";}i:1;N;}