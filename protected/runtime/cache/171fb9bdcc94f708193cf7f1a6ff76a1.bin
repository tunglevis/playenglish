a:2:{i:0;a:7:{s:2:"id";s:1:"9";s:5:"title";s:40:"Ngủ nhiều có thể gây đau đầu";s:4:"desc";s:427:"Đau đầu: Với những người dễ bị nhức đầu, ngủ nhiều hơn sẽ dễ gây ra đau đầu hơn. Các nhà khoa học cho rằng nguyên nhân là do ảnh hưởng của ngủ quá giấc lên các chất dẫn truyền thần kinh trong não, đặc biệt là serotonin. Ngủ nhiều vào ban ngày sẽ phá vỡ giấc ngủ ban đêm, và do đó họ thường bị nhức đầu vào sáng hôm sau.";s:7:"content";s:0:"";s:7:"created";s:8:"16/11/13";s:4:"type";s:7:"SUCKHOE";s:10:"type_label";s:12:"Sức khỏe";}i:1;N;}